package Alg;
import java.util.*;
public class TestEnvironment {
	
	static Scanner scan = new Scanner(System.in);
	
	public static void main(String[] args) {
		
		int N = scan.nextInt();
		
		if(N % 2 != 0) {
			System.out.println("Weird");
		}
		
		else if(N % 2 == 0 && (N <= 5 && N >= 2)) {
			System.out.println("Not Weird");
		}
		
		else if(N % 2 == 0 && (N <= 6 && N >= 20)) {
			System.out.println("Weird");
		}
		
		else if(N > 20) {
			System.out.println("Not Weird");
		}
	}

}
