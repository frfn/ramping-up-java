package DayTwoExercises;

public class exerciseTwo {
	
	public static void main(String[] args) { 
		
		//print phrase knowledge is power
		/*
		 * a. on one line
		 * b. on three lines, one word per line
		 * c. inside a box made of = and |
		 */
		
		System.out.println("Knowledge is Power");
		System.out.println();
		
		System.out.println("		Knowledge");
		System.out.println("		Is\n		Power");
		System.out.println();
		
		System.out.println("========================");
		System.out.println("|        Knowledge     |");
		System.out.println("|           Is         |");
		System.out.println("|          Power       |");
		System.out.println("========================");
		
	}

}
